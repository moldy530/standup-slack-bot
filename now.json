{
  "name": "Jamm",
  "version": 2,
  "env": {
    "SLACK_CLIENT_ID": "@slack-client-id",
    "SLACK_CLIENT_SECRET": "@slack-client-secret",
    "SLACK_BOT_TOKEN": "@slack-bot-token",
    "SLACK_SIGNING_SECRET": "@slack-signing-secret"
  },
  "routes": [
    { "methods": ["POST"], "src": "/slack/events", "dest": "/lib/app.js" }
  ],
  "builds": [{
    "src": "lib/*.js",
    "use": "@now/node@canary"
  }]
}
